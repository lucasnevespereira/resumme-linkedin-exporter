{
  "name": "Resumme LinkedIn Exporter",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Export LinkedIn Data to JSON format",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "cookies"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "./scripts/index.js"
      ],
      "matches": [
        "*://linkedin.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "sw/background.js"
  }
}


